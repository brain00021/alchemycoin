block countdown
	section#js-home.base_block(data-section="home")
		.row.transparent_bg
			.col-sm-7.col-xs-12
				.left_block.to-animate
					.video_box
						iframe(src="https://www.youtube.com/embed/FPo-IV_GiWw" allow="autoplay; encrypted-media" allowfulscreen frameBorder="0")
			.col-sm-5.col-xs-12
				.right_block.to-animate
					h3.heading 22% bonus finishes in:
					.timer
					.progress_warp
						span.f_left Raised
						span.f_right Next goal
						.progress_bg
							.progress
								.progress-bar(role="progressbar" aria-valuenow="58" aria-valuemin="0" aria-valuemax="100" style="min-width:58%")
									span 58
									span %
						span.f_left $5,807,401
						span.f_right $10,000,000
					button.coinBtn
						span BUY COINS NOW
					button.coinBtn.reverseBtn
						span READ WHITEPAPER
