block countdown
	.container
		.row.base_block
			.left_block.col-sm-7.col-xs-12
				.video_box
					iframe(src="https://www.youtube.com/embed/9NJYznYiLfI" allow="autoplay; encrypted-media" allowfulscreen)
			.right_block.col-sm-5.col-xs-12
				h3 22% bonus finishes in:
				.timer
				.progress_bg
					.progress
						.progress-bar(role="progressbar" aria-valuenow="58" aria-valuemin="0" aria-valuemax="100" style="min-width:58%")
							span 58
				button.coinBtn
					span BUT COINS NOW
				button.coinBtn.reverseBtn
					span READ WHITEPAPER
